# Copy this file to .env and fill in your secrets.
# This repo does not auto-load .env; export variables or load via your environment.

# Provider selection for Stage 2 chains (optional)
# LLM_PROVIDER=openai   # options: openai | groq | gemini | cohere
# LLM_MODEL=gpt-4o-mini # e.g., gpt-4o, groq/llama-3.1, gemini-1.5-pro, command-r-plus

# OpenAI API Key (optional)
OPENAI_API_KEY=

# Groq API Key (optional)
GROQ_API_KEY=

# Google Generative AI (Gemini) API Key (optional)
GOOGLE_API_KEY=

# Cohere API Key (optional)
COHERE_API_KEY=

# Preferred model (optional; used if you wire it up)
# PRIMARY_MODEL=gpt-4o-mini

# Router threshold (future stages)
# ROUTER_THRESHOLD=0.6

# Mask user identifiers (future guardrails)
# MASK_USER_IDS=true
